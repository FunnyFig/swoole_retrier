	    "ext-swoole": ">=4.4.2",
	    "funnyfig/swoole_thread": "dev-master",
	    "funnyfig/swoole_timer": "dev-master"
    },
    "autoload": {
	    "psr-4": {
		    "FunnyFig\\Swoole\\": "src"
	    }
